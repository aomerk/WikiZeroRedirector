{
  "name": "WikiZeroRedirect",
  "version": "0.8",
  "description": "Use Wikipedia in Banana Republics",
  "permissions": [

  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
  ,
  "content_scripts": [
    {
      "matches" : ["https://www.google.com/*"],
      "js": [
        "content.js"
      ]
    }
  ]
}
